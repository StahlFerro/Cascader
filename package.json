  },
  "homepage": "https://github.com/StahlFerro/TridentFrame#readme",
  "dependencies": {
    "file-uri-to-path": "^1.0.0",
    "fstream": "^1.0.12",
    "iv-viewer": "^2.0.1",
    "jsdom": "^15.0.0",
    "npm": "^6.10.3",
    "tar": "^4.4.9",
    "zerorpc": "git+https://github.com/fyears/zerorpc-node.git"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
