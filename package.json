{
  "name": "tridentframe",
  "version": "0.0.1",
  "description": "GIF/APNG manipulation tool",
  "main": "main.js",
  "scripts": {
    "clean": "rimraf dist",
    "css-build": "node-sass --omit-source-map-url sass/bulmamods.scss css/bulmamods.css",
    "css-watch": "npm run css-build -- --watch",
    "build": "npm run clean && webpack --mode production",
    "dev": "cross-env DEPLOY_ENV=DEV electron .",
    "prod": "electron .",
    "liverelease": "npm run clean && webpack --watch",
    "megafix": "npm install && npm install zeromq@5.1.0 && npm install electron@6.0.9 && npm install zerorpc && npm rebuild zeromq --runtime=electron && npm run rebuiltron",
    "rebuiltron": "./node_modules/.bin/electron-rebuild",
    "wpserve": "webpack-dev-server --mode development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StahlFerro/TridentFrame.git"
  },
  "author": "StahlFerro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/StahlFerro/TridentFrame/issues"
  },
  "homepage": "https://github.com/StahlFerro/TridentFrame#readme",
  "dependencies": {
    "file-uri-to-path": "^1.0.0",
    "fstream": "^1.0.12",
    "iv-viewer": "^2.0.1",
    "jsdom": "^15.1.1",
    "tar": "^4.4.10",
    "zeromq": "^5.1.0",
    "zerorpc": "^0.9.8"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "babel-loader": "^8.0.6",
    "bulma": "^0.7.5",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "electron": "^6.0.9",
    "electron-packager": "^9.0.1",
    "electron-rebuild": "^1.8.6",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "make-fetch-happen": "^6.0.1",
    "mini-css-extract-plugin": "^0.8.0",
    "node-loader": "^0.6.0",
    "node-sass": "^4.12.0",
    "rimraf": "^3.0.0",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.40.2",
    "webpack-bundle-analyzer": "^3.5.0",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0"
  }
}
